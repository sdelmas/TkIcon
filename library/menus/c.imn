tixPopupMenu $cbCanvasPath-c \
  -title "C-file"
set cbTmpPopupMenu [$cbCanvasPath-c subwidget menu]
$cbTmpPopupMenu add command \
  -command {StageShow C} \
  -label {Show}
$cbTmpPopupMenu add command \
  -command {StagePromote} \
  -label {Promote}
$cbTmpPopupMenu add command \
  -command {StageUnPromote} \
  -label {Unpromote}
$cbTmpPopupMenu add separator
$cbTmpPopupMenu add command \
  -command {StageSave} \
  -label {Save} 

